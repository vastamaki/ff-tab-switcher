{
  "manifest_version": 2,
  "name": "Cmd Shift Arrow Tab Switcher",
  "version": "1.0",
  "permissions": ["tabs"],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}
